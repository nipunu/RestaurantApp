@page
@model RestaurantApp.Pages.Restaurants.ListModel
@{
}
<h1>Restaurants</h1>

<table class="table">
    @foreach (var rest in Model.Restaurants)
    { 
    <tr>
        <td>@rest.Name</td>
        <td>@rest.Location</td>
        <td>@rest.Cuisine</td>
    </tr>
    }
</table>



<div>@Model.Message</div>
